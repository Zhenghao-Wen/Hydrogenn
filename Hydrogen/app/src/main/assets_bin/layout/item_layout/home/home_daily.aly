{
  LinearLayout,
  orientation="horizontal",
  layout_width="-1",
  BackgroundColor=backgroundc;
  {
    MaterialCardView;
    layout_margin="16dp";
    layout_marginTop="8dp";
    layout_marginBottom="8dp";
    layout_gravity='center';
    Elevation='0';
    layout_width='-1';
    layout_height='-2';
    radius=cardradius;
    CardBackgroundColor=backgroundc;
    StrokeColor=oricardedge;
    StrokeWidth=dp2px(1),
    id="card";
    {
      LinearLayout;
      layout_height="fill";
      id="background";
      layout_width="fill";

      {
        LinearLayout;
        orientation="horizontal";
        padding="16dp";
        {
          LinearLayout;
          orientation="vertical";
          {
            TextView;
            textColor=textc;
            Typeface=字体("product-Bold");
            id="标题",
            textSize=标题文字大小;
            lineHeight=标题行高;
          };
        };
      };
    };
  };
};